{
  "name": "verse/run",
  "description": "Php framework that provide easy way to create php micro-services",
  "keywords": ["framework", "microservices", "microservice"],
  "homepage": "http://github.com/vetermanve/verse_run/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Denisov",
      "email": "manwebdev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "monolog/monolog" : "*",
    "ralouphie/getallheaders": "^2.0",
    "verse/router": "^0.0.1",
    "verse/env": "^0.0.1",
    "verse/modular": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {"Verse\\Run\\": "src/Verse/Run"}
  },
  "autoload-dev": {
    "psr-4": {"Verse\\Run\\": "tests/Verse/Run"}
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
